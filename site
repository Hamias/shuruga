        const slides = document.querySelectorAll('.slide');
        const prevBtn = document.getElementById('prevBtn');
        const nextBtn = document.getElementById('nextBtn');
        const slideCounter = document.getElementById('slideCounter');
        let currentSlide = 0;
        const totalSlides = slides.length;

        const ceiData = {
            labels: ['Бренд', 'Окружение', 'Предложение', 'Культура'],
            datasets: [
                 { label: 'Ezgu Niyat', data: [2.5, 3.2, 1.8, 2.7], borderColor: '#e06666', backgroundColor: 'rgba(224, 102, 102, 0.2)'},
                 { label: 'Doctor D.', data: [4.0, 4.0, 4.0, 3.2], borderColor: '#93c47d', backgroundColor: 'rgba(147, 196, 125, 0.2)'},
                 { label: 'Akfa Medline', data: [3.4, 2.8, 2.2, 2.4], borderColor: '#ffd966', backgroundColor: 'rgba(255, 217, 102, 0.2)'}
            ]
        };
        
        const summaryJourneyData = {
            labels: ['Сайт', 'Колл-центр', 'Регистратура', 'Врач', 'Клиентский путь'],
            datasets: [
                {
                    label: 'Ezgu Niyat',
                    data: [2.3, 2.7, 3.1, 3.4, 2.3],
                    backgroundColor: '#e06666',
                    borderColor: '#e06666',
                    borderWidth: 1
                },
                {
                    label: 'Doctor D.',
                    data: [3.8, 3.2, 3.8, 3.2, 2.8],
                    backgroundColor: '#93c47d',
                    borderColor: '#93c47d',
                    borderWidth: 1
                },
                {
                    label: 'Akfa Medline',
                    data: [3.4, 2.6, 2.8, 3.2, 2.2],
                    backgroundColor: '#ffd966',
                    borderColor: '#ffd966',
                    borderWidth: 1
                }
            ]
        };

        let ceiChartInstance = null;
        let summaryJourneyChartInstance = null;

        function showSlide(index) {
            slides.forEach((slide, i) => slide.classList.toggle('active', i === index));
            slideCounter.textContent = `Слайд ${index + 1} из ${totalSlides}`;
            prevBtn.disabled = index === 0;
            nextBtn.disabled = index === totalSlides - 1;

            const activeSlide = slides[index];
            if (activeSlide) {
                activeSlide.querySelectorAll('.bar').forEach(bar => {
                    const width = bar.getAttribute('data-width');
                    setTimeout(() => { bar.style.width = width; }, 100);
                });
            }
            
            if (activeSlide && activeSlide.id === 'slide-4' && !summaryJourneyChartInstance) {
                 const ctx = document.getElementById('summaryJourneyChart').getContext('2d');
                 summaryJourneyChartInstance = new Chart(ctx, {
                    type: 'bar',
                    data: summaryJourneyData,
                    options: {
                        responsive: true,
                        maintainAspectRatio: false,
                        plugins: { legend: { display: true, position: 'top' } },
                        scales: {
                            y: { beginAtZero: true, max: 5, grid: { color: '#e0e0e0' }, ticks: { font: { size: 12, family: "'Open Sans', sans-serif" }} },
                            x: { grid: { display: false }, ticks: { font: { size: 14, family: "'Open Sans', sans-serif" }} }
                        }
                    }
                 });
            }

            if (activeSlide && activeSlide.id === 'slide-28' && !ceiChartInstance) {
                const ctx = document.getElementById('ceiChart').getContext('2d');
                ceiChartInstance = new Chart(ctx, { type: 'radar', data: ceiData, options: { responsive: true, maintainAspectRatio: false, elements: { line: { borderWidth: 3 } }, scales: { r: { beginAtZero: true, max: 5, pointLabels: { font: { size: 14, family: "'Open Sans', sans-serif" } }, grid: { color: '#ccc'}, angleLines: { color: '#ccc' } } }, plugins: { legend: { labels: { font: { size: 14, family: "'Open Sans', sans-serif" } } } } } });
            }
        }

        prevBtn.addEventListener('click', () => { if (currentSlide > 0) showSlide(--currentSlide); });
        nextBtn.addEventListener('click', () => { if (currentSlide < totalSlides - 1) showSlide(++currentSlide); });

        showSlide(currentSlide);
    </script>
</body>
</html>

